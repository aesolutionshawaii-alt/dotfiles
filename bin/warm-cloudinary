#!/bin/bash
# Warm Cloudinary CDN cache for Reel Addiction III
# Pre-generates image transformations for fast loading

PROJECT_DIR="$HOME/Desktop/reel-addiction-iii"

if [ ! -d "$PROJECT_DIR" ]; then
  echo "Error: Project not found at $PROJECT_DIR"
  exit 1
fi

cd "$PROJECT_DIR" && node warm-cloudinary.js "$@"
